 function TFrmmain.showword:string;
var
  s:string;
begin
  if wordlist.Count>0 then
  begin
    if rg1.ItemIndex=0 then
    begin
      randomize;
      wordrec:=1+Random(wordlist.Count) ;
    end
    else
    begin
      if wordrec<wordlist.Count then
      wordrec :=wordrec+1
      else
      wordrec :=1;
    end;
    s:=wordlist.Strings[wordrec-1];
    Result:=s;
  end;
end;

